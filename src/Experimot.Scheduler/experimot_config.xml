<?xml version="1.0" encoding="utf-8" ?>
<experimot_config xmlns="http://www.tuat.ac.jp/~venture/ExperimotConfig">
  <parameters>
    <param key="ApplicationName" value="ExperiMot" type="string"/>
    <param key="Author" value="Praveenkumar VASUDEVAN" type="string"/>
    <param key="Institution" value="GVLab" type="string"/>
    <param key="Description" value="An Experimental Platform for Human Robot Interaction based on Human Motion" type="string"/>
    <param key="ParameterServerHost" value="tcp://*" type="string"/>
    <param key="ParameterClientHost" value="tcp://localhost" type="string"/>
    <param key="ParameterServerPort" value="5560" type="int"/>
    <param key="ParameterServerInterval" value="10" type="int"/>
    <param key="WebServerEnabled" value="true" type="bool"/>
    <param key="WebServerPort" value="8888" type="int"/>
    <param key="WebServerHost" value="http://localhost" type="int"/>
    <param key="ROBOTIP" value="127.0.0.1" type="string"/>
    <param key="ROBOTPORT" value="51787" type="int"/>
    <!--<param key="ROBOTIP" value="192.168.11.3" type="string"/>
    <param key="ROBOTPORT" value="9559" type="int"/>-->
    <param key="ContextServerHost" value="tcp://*" type="string"/>
    <param key="ContextClientHost" value="tcp://localhost" type="string"/>
    <param key="ContextServerPort" value="5800" type="int"/>
    <param key="ContextServerInterval" value="10" type="int"/>
    <param key="MainProgramFilePath" value="%DEV_SDK_ROOT%\experimot\bin\scriptcs\main.csx" type="string"/>
    <param key="MainProgramFolder" value="%DEV_SDK_ROOT%\experimot\bin\scriptcs\" type="string"/>
    <!-- TODO Define Parameter for Periods of ContextServer, ContextSync, ParameterServer, GesturePeriod-->
  </parameters>
  <nodes>
    <node enabled="false" name="localization">
      <process>
        <type>executable</type>
        <path>%DEV_SDK_ROOT%\experimot\bin\experimot_localization.exe</path>
        <args></args>
      </process>
      <parameters>
        <param key="cube_size" value="80" type="int"/>
        <param key="marker_size" value="60" type="int"/>
        <param key="comm_timeout" value="100" type="int"/>
        <param key="calib_file" value="camera_calibration_kinect_1600.xml" type="string"/>
        <param key="sensor_process_cycle" value="20" type="int"/>
        <param key="robot_listener_cycle" value="40" type="int"/>
        <param key="pose_publisher_cycle" value="80" type="int"/>
        <param key="visualize" value="1" type="int"/>
        <param key="tdm_server_port" value="5700" type="int"/>
        <param key="tdm_server_cycle" value="40" type="int"/>
        <param key="max_new_marker_error" value="0.08" type="double"/>
        <param key="max_track_error" value="0.2" type="double"/>
        <param key="flip_image" value="0" type="bool"/>
        <param key="front_marker_flip_id" value="3" type="int"/>
        <param key="left_marker_flip_id" value="0" type="int"/>
        <param key="right_marker_flip_id" value="2" type="int"/>
        <param key="rear_marker_flip_id" value="1" type="int"/>
        <param key="top_marker_flip_id" value="5" type="int"/>
        <param key="front_marker_id" value="7" type="int"/>
        <param key="left_marker_id" value="0" type="int"/>
        <param key="right_marker_id" value="13" type="int"/>
        <param key="rear_marker_id" value="10" type="int"/>
        <param key="top_marker_id" value="14" type="int"/>
        <param key="world_marker_id" value="160" type="int"/>
        <param key="world_marker_size" value="198" type="int"/>
      </parameters>
      <publishers>
        <publish>
          <host>tcp://*</host>
          <port>5566</port>
          <topic>TPP</topic>
          <msg_type>Pose_V</msg_type>
          <name>TorsoPosePublisher</name>
        </publish>
      </publishers>
      <subscribers>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5563</port>
          <topic>RSP</topic>
          <msg_type>JointValueVector</msg_type>
          <name>RobotStateListener</name>
        </subscribe>
      </subscribers>
    </node>
    <node enabled="true" name="gesture_recognition">
      <process>
        <type>executable</type>
        <path>%DEV_SDK_ROOT%\experimot\bin\nodes\Experimot.Kinect.Perception\Experimot.Kinect.Perception.exe</path>
        <args></args>
      </process>
      <parameters>
        <param key="nao_joints" type="file" value="nao_joints_h25v50.json"/>
        <param key="nao_kinect" type="file" value="kinect_nao_map.json"/>
        <param key="database" type="file" value="Database\experimot.gbd,Database\Seated.gbd"/>
      </parameters>
      <publishers>
        <publish>
          <host>tcp://*</host>
          <port>5570</port>
          <topic>KSP</topic>
          <msg_type>KinectBodies</msg_type>
          <name>KinectBodyPublisher</name>
        </publish>
        <publish>
          <host>tcp://*</host>
          <port>5580</port>
          <topic>GTP</topic>
          <msg_type>GestureTriggers</msg_type>
          <name>GesturePublisher</name>
        </publish>
        <publish>
          <host>tcp://*</host>
          <port>5571</port>
          <topic>HPP</topic>
          <msg_type>Humans</msg_type>
          <name>HumanPosePublisher</name>
        </publish>
        <publish>
          <host>tcp://*</host>
          <port>5592</port>
          <topic>NJP</topic><!--Nao Joint Publisher-->
          <msg_type>ParamList</msg_type>
          <name>NaoJointPublisher</name>
        </publish>
      </publishers>
    </node>
    <node enabled="false" name="speech_recognition">
      <process>
        <type>executable</type>
        <path>%DEV_SDK_ROOT%\experimot\bin\nodes\Experimot.Kinect.Speech\Experimot.Kinect.Speech.exe</path>
        <args></args>
      </process>
      <parameters>
        <param key="ConfidenceThreshold" type="double" value="0.60"/>
        <param key="DegreesInRightAngle" type="int" value="90"/>
        <param key="DisplacementAmount" type="int" value="60"/>
      </parameters>
    </node>
    <node enabled="false" name="simulation">
      <process>
        <type>executable</type>
        <path>%DEV_SDK_ROOT%\experimot\bin\experimot_rave.exe</path>
        <args></args>
      </process>
      <parameters>
        <param key="scenefilename" value="data/nao.dae" type="string"/>
        <param key="kinect_model" value="box2.kinbody.xml" type="string"/>
        <param key="viewername" value="qtcoin" type="string"/>
        <param key="visualize" value="1" type="int"/>
      </parameters>
      <subscribers>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5563</port>
          <topic>RSP</topic>
          <msg_type>JointValueVector</msg_type>
          <name>RobotStateListener</name>
        </subscribe>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5566</port>
          <topic>TPP</topic>
          <msg_type>Pose_V</msg_type>
          <name>TorsoPoseListener</name>
        </subscribe>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5570</port>
          <topic>KSP</topic>
          <msg_type>KinectBodies</msg_type>
          <name>KinectStateListener</name>
        </subscribe>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5580</port>
          <topic>GTP</topic>
          <msg_type>GestureTriggers</msg_type>
          <name>GesturePublisher</name>
        </subscribe>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5571</port>
          <topic>HPP</topic>
          <msg_type>Humans</msg_type>
          <name>HumanPosePublisher</name>
        </subscribe>
      </subscribers>
    </node>
    <node enabled="false" name="localization_logger">
      <process>
        <type>executable</type>
        <path>%DEV_SDK_ROOT%\experimot\bin\nodes\Experimot.Localization.Logger\Experimot.Localization.Logger.exe</path>
        <args></args>
      </process>
      <parameters>
        <param key="tdm_server_port" value="5700" type="int"/>
      </parameters>
      <subscribers>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5566</port>
          <topic>TPP</topic>
          <msg_type>Pose_V</msg_type>
          <name>TorsoPoseListener</name>
        </subscribe>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5571</port>
          <topic>HPP</topic>
          <msg_type>Humans</msg_type>
          <name>HumanPosePublisher</name>
        </subscribe>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5570</port>
          <topic>KSP</topic>
          <msg_type>KinectBodies</msg_type>
          <name>KinectBodyPublisher</name>
        </subscribe>
      </subscribers>
    </node>
    <node enabled="false" name="robot_interface">
      <process>
        <type>executable</type>
        <path>python.exe</path>
        <args>%DEV_SDK_ROOT%\experimot\scripts\experimot_robot_interface\ReadJointValues.py</args>
      </process>
      <parameters>
        <!--<param key="ROBOTIP" value="192.168.11.2" type="string"/>
        <param key="PORT" value="9559" type="int"/>-->
      </parameters>
      <publishers>
        <publish>
          <host>tcp://*</host>
          <port>5563</port>
          <topic>RSP</topic>
          <msg_type>JointValueVector</msg_type>
          <name>RobotStatePublisher</name>
        </publish>
      </publishers>
    </node>
    <node enabled="false" name="tdm_client">
      <process>
        <type>executable</type>
        <path>python.exe</path>
        <args>%DEV_SDK_ROOT%\experimot\scripts\experimot_python_client\experimot_relative_localize.py</args>
      </process>
    </node>
    <node enabled="false" name="nao_behavior_manager">
      <process>
        <type>executable</type>
        <path>python.exe</path>
        <args>%DEV_SDK_ROOT%\experimot\scripts\experimot_robot_interface\behaviors\behaviormanager.py</args>
      </process>
      <parameters>
        <param key="RequestServerIP" value="tcp://*" type="string"/>
        <param key="RequestClientIP" value="tcp://localhost" type="string"/>
        <param key="RequestServerPort" value="5590" type="int"/>
      </parameters>
    </node>
    <node enabled="false" name="nao_imitation_manager">
      <process>
        <type>executable</type>
        <path>python.exe</path>
        <args>%DEV_SDK_ROOT%\experimot\scripts\experimot_robot_interface\behaviors\imitation_manager.py</args>
      </process>
      <parameters>
        <param key="RequestServerIP" value="tcp://*" type="string"/>
        <param key="RequestClientIP" value="tcp://localhost" type="string"/>
        <param key="RequestServerPort" value="5590" type="int"/>
      </parameters>
      <subscribers>
        <subscribe>
          <host>tcp://localhost</host>
          <port>5592</port>
          <topic>NJP</topic><!--Nao Joint Publisher-->
          <msg_type>ParamList</msg_type>
          <name>NaoJointPublisher</name>
        </subscribe>
      </subscribers>
    </node>
    <node enabled="false" name="main_program">
      <process>
        <type>executable</type>
        <path>python.exe</path>
        <args>%DEV_SDK_ROOT%\experimot\scripts\experimot_python_client\aps_test.py</args>
      </process>
      <parameters>
        <param key="BehaviorManagerName" value="nao_behavior_manager" type="string"/>
      </parameters>
    </node>
    <node enabled="false" name="scriptcs_program">
      <process>
        <type>scriptcs</type>
        <path>%DEV_SDK_ROOT%\experimot\bin\scriptcs\scriptcs.exe</path>
        <args>%DEV_SDK_ROOT%\experimot\bin\scriptcs\main.csx -logLevel debug</args>
      </process>
      <parameters>
        <param key="BehaviorManagerName" value="nao_behavior_manager" type="string"/>
      </parameters>
    </node>
  </nodes>
</experimot_config>